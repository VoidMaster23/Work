  <application
      android:allowBackup="true"
      android:usesCleartextTraffic="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <uses-library android:name="org.apache.http.legacy" android:required="false"/>

    <activity android:name=".Time_Sheet2"></activity>
    <activity android:name=".VehicleCheckList" />
    <activity android:name=".Ambulance" />
    <activity android:name=".FireEngine" />
    <activity android:name=".FireSelect" />
    <activity android:name=".FireSkid" />
    <activity android:name=".ILSAMB" />
    <activity android:name=".ALS_ICU_AMB" />
    <activity android:name=".ALSRV" />
    <activity android:name=".ALSAMB_PAED" />
    <activity android:name=".medicalTabbedView" />
    <activity android:name=".categoryType" />
    <activity android:name=".MedicalForm" />
    <activity android:name=".BroadCastMessage" />
    <activity android:name=".ChooseBroadCast" />
    <activity android:name=".ViewAttendanceCrew" />
    <activity android:name=".BroadCast" />
    <activity android:name=".ViewAttendance" />
    <activity android:name=".TimeSheet" />
    <activity android:name=".Home_Screen_Manager" />
    <activity android:name=".Signature" />
    <activity
        android:name=".SwapShift"
        android:label="@string/title_activity_swap_shift" />
    <activity android:name=".DashBoard" />
    <activity android:name=".Home_Screen_Crew" />
    <activity android:name=".Login_Page">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>
  <uses-permission android:name="android.permission.INTERNET" />
  <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
  <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
  <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

  <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

</manifest>