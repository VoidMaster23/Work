<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/back"
    tools:context=".SampleHistory"
    android:padding="15dp">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/logoContainer"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_alignParentTop="true">

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="184dp"
                    android:layout_height="150dp"
                    android:src="@drawable/logo"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true" />

                <CheckBox
                    android:id="@+id/notApplicableCheckBox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Not Applicable"
                    android:textSize="18sp"
                    android:textColor="@color/white"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_toStartOf="@id/imageView12"
                    android:layout_toLeftOf="@id/imageView12" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/lblSigns"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="8dp"
                    android:text="Signs and Symptoms"
                    android:textColor="@color/white"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true" />

                <EditText
                    android:id="@+id/edtSigns"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:height="50dp"
                    android:ems="10"
                    android:gravity="top|left"
                    android:hint="enter here"
                    android:inputType="textMultiLine|textPersonName"
                    android:isScrollContainer="true"
                    android:padding="5dp"
                    android:singleLine="false"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:layout_toEndOf="@id/lblSigns"
                    android:layout_toRightOf="@id/lblSigns"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true" />

                <EditText
                    android:id="@+id/edtAllergies"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="33dp"
                    android:ems="10"
                    android:gravity="top|left"
                    android:hint="enter here"
                    android:inputType="textMultiLine|textPersonName"
                    android:isScrollContainer="true"
                    android:paddingTop="5dp"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:layout_below="@+id/edtSigns"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignStart="@id/edtSigns"
                    android:layout_alignLeft="@id/edtSigns" />

                <TextView
                    android:id="@+id/lblAllergies"
                    android:layout_width="67dp"
                    android:layout_height="19dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="56dp"
                    android:ems="10"
                    android:gravity="top|left"
                    android:text="Allergies"
                    android:textColor="@color/white"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/lblSigns"
                    android:layout_alignParentLeft="true" />

                <EditText
                    android:id="@+id/edtMedication"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="33dp"
                    android:ems="10"
                    android:gravity="top|left"
                    android:hint="enter here"
                    android:inputType="textMultiLine|textPersonName"
                    android:isScrollContainer="true"
                    android:paddingTop="5dp"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:layout_below="@+id/edtAllergies"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignStart="@id/edtSigns"
                    android:layout_alignLeft="@id/edtSigns"/>

                <EditText
                    android:id="@+id/edtPast"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="33dp"
                    android:ems="10"
                    android:gravity="top|left"
                    android:hint="enter here"
                    android:inputType="textMultiLine|textPersonName"
                    android:isScrollContainer="true"
                    android:paddingTop="5dp"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:layout_below="@+id/edtMedication"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignStart="@id/edtSigns"
                    android:layout_alignLeft="@id/edtSigns"/>


                <EditText
                    android:id="@+id/edtIntake"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="33dp"
                    android:ems="10"
                    android:gravity="top|left"
                    android:hint="enter here"
                    android:inputType="textMultiLine|textPersonName"
                    android:isScrollContainer="true"
                    android:paddingTop="5dp"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:layout_below="@+id/edtPast"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignStart="@id/edtSigns"
                    android:layout_alignLeft="@id/edtSigns"/>

                <EditText
                    android:id="@+id/edtEvents"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="33dp"
                    android:ems="10"
                    android:gravity="top|left"
                    android:hint="enter here"
                    android:inputType="textMultiLine|textPersonName"
                    android:isScrollContainer="true"
                    android:paddingTop="5dp"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:layout_below="@+id/edtIntake"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignStart="@id/edtSigns"
                    android:layout_alignLeft="@id/edtSigns" />

                <TextView
                    android:id="@+id/lblMedication"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="65dp"
                    android:text="Medications"
                    android:textColor="@color/white"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/lblAllergies"
                    android:layout_alignParentLeft="true" />

                <TextView
                    android:id="@+id/lblPast"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="65dp"
                    android:text="Past Medical Hx"
                    android:textColor="@color/white"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/lblMedication"
                    android:layout_alignParentLeft="true" />

                <TextView
                    android:id="@+id/lblIntake"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="65dp"
                    android:text="Last Intake/Output"
                    android:textColor="@color/white"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/lblPast"
                    android:layout_alignParentLeft="true" />

                <TextView
                    android:id="@+id/lblEvents"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="65dp"
                    android:text="Events leading up to"
                    android:textColor="@color/white"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/lblIntake"
                    android:layout_alignParentLeft="true" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>